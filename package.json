{
  "name": "boilerplate-sls-lambda-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && npx tsc -p tsconfig.build.json",
    "deploy": "./deploy.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node dist/index.js",
    "start:build": "npm run build && npm start",
    "test": "npm run test:unit && npm run test:integration",
    "test:acceptance": "./acceptance-tests.sh",
    "test:all": "npm run test && npm run test:acceptance",
    "test:coverage": "npx nyc npm run test:all",
    "test:integration": "mocha test/integration/**/*.spec.ts",
    "test:unit": "mocha test/unit/**/*.spec.ts",
    "serverless:deploy": "serverless deploy"
  },
  "pre-commit": [
    "lint",
    "test:unit",
    "test:integration"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "express": "^4.17.1",
    "serverless-http": "^2.6.0",
    "ts-lib": "0.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.7",
    "@types/serverless": "^1.78.9",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.15.1",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "mocha": "^8.2.1",
    "serverless": "^2.11.1",
    "serverless-webpack": "^5.3.5",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "supertest": "^6.0.1",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0"
  }
}
